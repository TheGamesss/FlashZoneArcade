<svg width="64" height="64" viewBox="0 0 64 64" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- Gradient for letters -->
    <linearGradient id="iconGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#fff14d"/>
      <stop offset="25%" stop-color="#ffcc33"/>
      <stop offset="50%" stop-color="#ffaa00"/>
      <stop offset="75%" stop-color="#ff8800"/>
      <stop offset="100%" stop-color="#ff6600"/>
    </linearGradient>

    <!-- Shadow extrusion for 3D depth -->
    <linearGradient id="iconShadow" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#000" stop-opacity="0.5"/>
      <stop offset="100%" stop-color="#000" stop-opacity="0"/>
    </linearGradient>

    <!-- Neon glow behind letters -->
    <radialGradient id="iconGlow" cx="50%" cy="50%" r="50%">
      <stop offset="0%" stop-color="#ffcc33" stop-opacity="0.35"/>
      <stop offset="50%" stop-color="#ff33ff" stop-opacity="0.15"/>
      <stop offset="100%" stop-color="#ff33ff" stop-opacity="0"/>
    </radialGradient>

    <!-- Gloss overlay -->
    <linearGradient id="iconGloss" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#fff" stop-opacity="0.35"/>
      <stop offset="50%" stop-color="#fff" stop-opacity="0.1"/>
      <stop offset="100%" stop-color="#fff" stop-opacity="0"/>
    </linearGradient>
  </defs>

  <!-- Outer border -->
  <rect x="2" y="2" width="60" height="60" rx="10" fill="#222" stroke="#000" stroke-width="2"/>

  <!-- Neon glow -->
  <rect x="2" y="2" width="60" height="60" rx="10" fill="url(#iconGlow)"/>

  <!-- Shadow extrusion behind letters (offset slightly) -->
  <text x="34" y="48" font-family="'Press Start 2P', sans-serif" font-size="36" font-weight="900" text-anchor="middle" fill="url(#iconShadow)">
    FZ
  </text>

  <!-- Main letters with gradient -->
  <text x="31.45" y="45" font-family="'Press Start 2P', sans-serif" font-size="36" font-weight="900" text-anchor="middle" fill="url(#iconGrad)" stroke="#000" stroke-width="1.5">
    FZ
  </text>

  <!-- Gloss overlay with bigger border radius -->
  <rect x="4" y="4" width="56" height="30" rx="8" fill="url(#iconGloss)"/>
</svg>
